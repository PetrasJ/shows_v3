{% extends 'base.html.twig' %}

{% block title %}{{ 'forgot_password'|trans }}{% endblock %}

{% block body %}
    <h1>{{ 'forgot_password'|trans }}</h1>
    {{ form_start(forgotPasswordForm) }}
    {{ form_row(forgotPasswordForm.email) }}
    <button class="btn btn-primary">{{ 'send'|trans }}</button>
    {{ form_end(forgotPasswordForm) }}
{% endblock %}
